{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Product management microservice for fashion e-commerce",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "export:db": "node utils/exportDatabase.js",
    "import:db": "node utils/importDatabase.js",
    "import:db:clear": "node utils/importDatabase.js --clear",
    "import:products": "node utils/importProducts.js",
    "import:sample": "node utils/importProducts.js data/sample-products.json",
    "upload:images": "node utils/bulkUploadImages.js",
    "update:prices": "node utils/updateProductPrices.js",
    "view:export": "node utils/viewExport.js",
    "check:variants": "node utils/checkVariants.js",
    "view:variants": "node utils/viewProductsWithVariants.js",
    "import:variants": "node utils/importVariants.js"
  },
  "dependencies": {
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "microservice",
    "product",
    "catalog",
    "ecommerce"
  ],
  "author": "Fashion E-commerce Team",
  "license": "MIT"
}
