    {
      "name": "2. Get Similar Products (POST)",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json",
            "type": "text"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n    \"productId\": \"{{test_product_id}}\",\n    \"limit\": 6,\n    \"options\": {\n        \"sameCategoryOnly\": true,\n        \"priceTolerance\": 0.5,\n        \"filterGender\": true,\n        \"filterUsage\": true,\n        \"brandBoost\": 0.05,\n        \"minSimilarity\": 0.6\n    }\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/recommendations/similar",
          "host": ["{{base_url}}"],
          "path": ["api", "recommendations", "similar"]
